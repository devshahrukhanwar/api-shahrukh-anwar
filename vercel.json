{
	"version": 2,
	"buildCommand": "npm run build",
	"outputDirectory": "src",
	"builds": [
		{
			"src": "src/index.js",
			"use": "@vercel/node"
		}
	],
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "/src"
		}
	],
	"crons": [
		{
			"path": "/api/v1/clear/cache",
			"schedule": "0 10 * * *"
		}
	]
}